# Uname
CONFIG_UNAME_OVERRIDE=y
CONFIG_UNAME_OVERRIDE_TARGET="com.google.android.gms"
CONFIG_UNAME_OVERRIDE_STRING="4.19.157-perf+"

# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y
CONFIG_QCA_CLD_WLAN_PROFILE="qca6390"

# DebugFS
CONFIG_DEBUG_FS=n
CONFIG_MSM_TZ_LOG=n
CONFIG_CMA_DEBUGFS=n
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_IPA_UT=n
CONFIG_IOMMU_TESTS=n
CONFIG_IOMMU_TESTS=n
CONFIG_QMP_DEBUGFS_CLIENT=n

# exFAT
CONFIG_EXFAT_FS=y

# Persistent store
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# Backlight
CONFIG_BACKLIGHT_KTZ8866=y
CONFIG_BACKLIGHT_DUALKTZ8866=y

# Camera motor
CONFIG_MOTOR_DRV8846=y

# GPS
CONFIG_AIROHA_GPS=y

# Hall sensor
CONFIG_HALL_AKM09970=y

# ION
CONFIG_ION_CAMERA_HEAP=y

# IR
CONFIG_RC_CORE=y
CONFIG_LIRC=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI_MI=y

# Keyboard
CONFIG_INPUT_XIAOMI_KEYBOARD=y
CONFIG_KEYBOARD_NANO_8030=y

# LED
CONFIG_LEDS_AW2015=y

# Power
CONFIG_FRAME_WARN=4096
CONFIG_SMB1355_SLAVE_CHARGER=n
CONFIG_SMB1390_CHARGE_PUMP_PSY=n
CONFIG_SMB1398_CHARGER=n

# Regulator
CONFIG_I2C_RETRY=y
CONFIG_REGULATOR_PM8008=y
CONFIG_REGULATOR_QCOM_RPMH=y

# Touchscreen
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=n
CONFIG_TOUCHSCREEN_SYNAPTICS_TCM=n
CONFIG_TOUCHSCREEN_ST=n
CONFIG_TOUCHSCREEN_COMMON=y

# USB
CONFIG_PS5169=y

# Vibrator
CONFIG_INPUT_QTI_HAPTICS=n

# Coresight
CONFIG_CORESIGHT=n
CONFIG_CORESIGHT_PLACEHOLDER=y
CONFIG_CORESIGHT_AMBA_PLACEHOLDER=y

# Modules
CONFIG_MODULES=n

# Audit
CONFIG_AUDIT=n

# Simple Low Memory Killer
CONFIG_PSI=n
CONFIG_MEMCG=n
CONFIG_MEMCG_SWAP=n
CONFIG_ANDROID_SIMPLE_LMK=y

# KProfiles
CONFIG_DEFAULT_KP_MODE=1
CONFIG_AUTO_KPROFILES=y
CONFIG_AUTO_KPROFILES_MI_DRM=y

# Devfreq boost
CONFIG_DEVFREQ_BOOST=y
CONFIG_DEVFREQ_INPUT_BOOST_DURATION_MS=64
CONFIG_DEVFREQ_WAKE_BOOST_DURATION_MS=0
CONFIG_DEVFREQ_MSM_CPU_DDR_BW_BOOST_FREQ=3879

# CPU Boost & MSM Performance
CONFIG_CPU_BOOST=n
CONFIG_MSM_PERFORMANCE=n

# CPU masks
CONFIG_LITTLE_CPU_MASK=15
CONFIG_BIG_CPU_MASK=112
CONFIG_PRIME_CPU_MASK=128

# Security
CONFIG_SECURITY_APPARMOR=n

# Crypto
CONFIG_CRYPTO_LZ4=y

# EroFS
CONFIG_EROFS_FS=y
CONFIG_EROFS_FS_XATTR=y
CONFIG_EROFS_FS_POSIX_ACL=y
CONFIG_EROFS_FS_SECURITY=y
CONFIG_EROFS_FS_ZIP=y

# Memory
CONFIG_PROCESS_RECLAIM=n

# Debug
CONFIG_DEBUG_KERNEL=n
CONFIG_IKHEADERS=n
CONFIG_ARM64_PAN=n
CONFIG_IPC_LOGGING=n

# Cpufreq
CONFIG_CPU_FREQ_TIMES=n
CONFIG_CPU_FREQ_STAT=y
CONFIG_CPU_FREQ_GOV_POWERSAVE=n
CONFIG_CPU_FREQ_GOV_USERSPACE=n
CONFIG_CPU_FREQ_GOV_ONDEMAND=n
CONFIG_CPU_FREQ_GOV_CONSERVATIVE=n

# Compression
CONFIG_RD_LZ4=y
CONFIG_RD_LZMA=n
CONFIG_F2FS_FS_COMPRESSION=y
CONFIG_F2FS_FS_LZ4=y
CONFIG_F2FS_FS_LZO=n
CONFIG_F2FS_FS_LZ4HC=n
CONFIG_F2FS_FS_ZSTD=n

# Jump label
CONFIG_JUMP_LABEL=y

# Profiling
CONFIG_PROFILING=n
CONFIG_PERF_EVENTS=y

# I/O schedulers
CONFIG_IOSCHED_NOOP=n
CONFIG_IOSCHED_DEADLINE=n
CONFIG_MQ_IOSCHED_DEADLINE=n
CONFIG_MQ_IOSCHED_KYBER=n

# Timer frequency
CONFIG_HZ_100=y

# Log buffer
CONFIG_LOG_BUF_SHIFT=20

# Suspend skip sync
CONFIG_SUSPEND_SKIP_SYNC=y

# TCP WestWood
CONFIG_TCP_CONG_ADVANCED=y
CONFIG_TCP_CONG_WESTWOOD=y

# Frame Warn
CONFIG_FRAME_WARN=0

# CASS
CONFIG_SCHED_CASS=y

# Disable WALT
CONFIG_SCHED_CORE_CTL=n
CONFIG_SCHED_WALT=n
